<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="MSCRM_DataSource"><DataSourceReference>MSCRM_DataSource</DataSourceReference><rd:SecurityType>Integrated</rd:SecurityType><rd:DataSourceID>f5f45726-4d1f-4911-b631-887ba67fef1c</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Person"><Query><DataSourceName>MSCRM_DataSource</DataSourceName><CommandText>SELECT CRMAF_FilteredContact.fullname
	,CRMAF_FilteredContact.elcn_dateofbirth
	,CRMAF_FilteredContact.elcn_preferredaddressname
	,CRMAF_FilteredContact.elcn_preferredemailaddressname
	,CRMAF_FilteredContact.elcn_preferredphonename
	,CRMAF_FilteredContact.elcn_primaryid
	,FORMAT(CRMAF_FilteredContact.elcn_totalamountreceived, 'C') AS elcn_totalamountreceived
	,FORMAT(CRMAF_FilteredContact.elcn_totalgiving, 'C') AS elcn_totalgiving
	,CRMAF_FilteredContact.elcn_consecutiveyearsofgiving
	,SpouseRecord.fullname AS SpouseName
	,SpouseRecord.elcn_primaryid AS SpouseId
	,CRMAF_FilteredContact.contactid
                 ,Convert(date, getdate())   as currentdate
	,FORMAT(CRMAF_FilteredContact.elcn_lastcontributionamount, 'C') AS elcn_lastcontributionamount
	,CRMAF_FilteredContact.elcn_lastcontributiondate
	,CRMAF_FilteredContact.elcn_lastcontributiontypename
	,FORMAT(CRMAF_FilteredContact.elcn_largestcontributionamount, 'C') AS elcn_largestcontributionamount
	,CRMAF_FilteredContact.elcn_largestcontributiondate
	,CRMAF_FilteredContact.elcn_largestcontributiontypename
	,CASE WHEN CRMAF_FilteredContact.elcn_confirmeddeathdate IS NULL AND CRMAF_FilteredContact.elcn_notifieddeathdate IS NULL then 'No' else 'Yes' end as is_dead
    ,CASE 
	     WHEN CRMAF_FilteredContact.elcn_yearofbirth IS NOT NULL AND CRMAF_FilteredContact.elcn_dayofbirth IS NOT NULL AND CRMAF_FilteredContact.elcn_monthofbirth IS NOT NULL THEN DATEDIFF(hour,CRMAF_FilteredContact.birthdate,GETDATE())/8766
		 WHEN CRMAF_FilteredContact.elcn_yearofbirth IS NOT NULL AND CRMAF_FilteredContact.elcn_dayofbirth IS NULL AND CRMAF_FilteredContact.elcn_monthofbirth IS NOT NULL THEN DATEDIFF(hour,CRMAF_FilteredContact.elcn_monthofbirth+'/01/'+CRMAF_FilteredContact.elcn_yearofbirth,GETDATE())/8766	 
		 WHEN CRMAF_FilteredContact.elcn_yearofbirth IS NOT NULL AND CRMAF_FilteredContact.elcn_dayofbirth IS NULL AND CRMAF_FilteredContact.elcn_monthofbirth IS NULL THEN DATEDIFF(hour,'01/01/'+CRMAF_FilteredContact.elcn_yearofbirth,GETDATE())/8766
		 ELSE NULL
		 END AS AgeYearsIntTrunc
FROM FilteredContact AS CRMAF_FilteredContact
	LEFT OUTER JOIN Filteredelcn_personalrelationship personalrelationship
		    Inner join FilteredContact SpouseRecord on personalrelationship.elcn_person2id = SpouseRecord.contactid
			Inner Join Filteredelcn_personalrelationshiptype relationshiptype on personalrelationship.elcn_RelationshipType1Id= relationshiptype.elcn_personalrelationshiptypeId
		ON CRMAF_FilteredContact.contactid = personalrelationship.elcn_person1id AND relationshiptype.elcn_IsSpousal = 1</CommandText><Timeout>3</Timeout><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="fullname"><DataField>fullname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SpouseName"><DataField>SpouseName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_dateofbirth"><DataField>elcn_dateofbirth</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_preferredemailaddressname"><DataField>elcn_preferredemailaddressname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_totalamountreceived"><DataField>elcn_totalamountreceived</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_totalgiving"><DataField>elcn_totalgiving</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_primaryid"><DataField>elcn_primaryid</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_consecutiveyearsofgiving"><DataField>elcn_consecutiveyearsofgiving</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="elcn_preferredaddressname"><DataField>elcn_preferredaddressname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_preferredphonename"><DataField>elcn_preferredphonename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_lastcontributionamount"><DataField>elcn_lastcontributionamount</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_lastcontributiondate"><DataField>elcn_lastcontributiondate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="currentdate"><DataField>currentdate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="elcn_lastcontributiontypename"><DataField>elcn_lastcontributiontypename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Fie