    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MSCRM_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_asofdate">
            <Value>=Parameters!p_asofdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_contributiontype">
            <Value>=Parameters!p_contributiontype.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @p_fiscalmonth INT
DECLARE @lv_curr_fy [nvarchar](4)
DECLARE @lv_prev1_fy  [nvarchar](4)
DECLARE @lv_prev2_fy  [nvarchar](4)
DECLARE @lv_calendar_year_ind [nvarchar](1)



--test the query in SQL Server management studio
--DECLARE @p_asofdate DATETIME
--SET @p_asofdate = CAST('01-MAR-2020' AS date)

SELECT @p_fiscalmonth = MONTH(fo.fiscalcalendarstart) FROM filteredorganization fo
;
SET @lv_calendar_year_ind = 'Y'

IF @lv_calendar_year_ind = 'N'
BEGIN
	SET @lv_curr_fy =  CASE WHEN MONTH(@p_asofdate) &gt;= @p_fiscalmonth THEN YEAR(@p_asofdate) + 1  ELSE YEAR(@p_asofdate) END
	SET @lv_prev1_fy =  CASE WHEN MONTH(@p_asofdate) &gt;= @p_fiscalmonth THEN YEAR(@p_asofdate) + 1 ELSE YEAR(@p_asofdate) END - 1
	SET @lv_prev2_fy =  CASE WHEN MONTH(@p_asofdate) &gt;= @p_fiscalmonth THEN YEAR(@p_asofdate) + 1  ELSE YEAR(@p_asofdate) END - 2
END
ELSE
BEGIN
	SET @lv_curr_fy = YEAR(@p_asofdate)  
	SET @lv_prev1_fy = YEAR(@p_asofdate) - 1 
	SET @lv_prev2_fy = YEAR(@p_asofdate) - 2 
END
;
--SELECT @p_fiscalmonth, @lv_curr_fy, @lv_prev1_fy, @lv_prev2_fy




--	NDN - No Donation Solicitations
SELECT *
INTO #tmp_exclusions
FROM
(
		SELECT cpb.elcn_personid, cpb.elcn_organizationid, 'NDN' exclusion_type 
		FROM elcn_contactpreferenceBase cpb
		WHERE cpb.elcn_ContactPreferenceTypeId = 'e4e02dc6-3314-e511-9431-005056804b43' /*Solicitations*/
		AND cpb.elcn_ContactRestrictionId = '8872A718-5472-40C4-82C7-DB72FC4CE5A6' /*Exclude*/
		AND (cpb.elcn_RestrictionLiftDate &lt; CURRENT_TIMESTAMP OR cpb.elcn_RestrictionLiftDate IS NULL)
		AND cpb.elcn_ContactPreferenceStatusId = '378DE114-EB09-E511-943C-0050568068B7' /*Current*/
		AND cpb.elcn_MethodofContact = 344220006 /*All*/
		
--	NMC - No Mail Contact
        UNION ALL

		SELECT cpb.elcn_personid, cpb.elcn_organizationid, 'NMC' exclusion_type 
		FROM elcn_contactpreferenceBase cpb
		WHERE cpb.elcn_ContactPreferenceTypeId = '112A7585-A2D9-E911-80D8-0A253F89019C' /*Communications*/
		AND cpb.elcn_ContactRestrictionId = '8872A718-5472-40C4-82C7-DB72FC4CE5A6' /*Exclude*/
		AND (cpb.elcn_RestrictionLiftDate &lt; CURRENT_TIMESTAMP OR cpb.elcn_RestrictionLiftDate IS NULL)
		AND cpb.elcn_ContactPreferenceStatusId = '378DE114-EB09-E511-943C-0050568068B7' /*Current*/
		AND cpb.elcn_MethodofContact = 344220000 /*Letter*/


--	NOC - No Contact

		UNION ALL

		SELECT cpb.elcn_personid, cpb.elcn_organizationid, 'NOC' exclusion_type 
		FROM elcn_contactpreferenceBase cpb
		WHERE cpb.elcn_ContactPreferenceTypeId = '76EA8AA5-2F36-4E8E-BFB2-490677DCF4B4' /*Global Restriction*/
		AND cpb.elcn_ContactRestrictionId = '8872A718-5472-40C4-82C7-DB72FC4CE5A6' /*Exclude*/
		AND (cpb.elcn_RestrictionLiftDate &lt; CURRENT_TIMESTAMP OR cpb.elcn_RestrictionLiftDate IS NULL)
		AND cpb.elcn_ContactPreferenceStatusId = '378DE114-EB09-E511-943C-0050568068B7' /*Current*/
		AND cpb.elcn_MethodofContact = 344220006 /*All*/
) fq
;

CREATE NONCLUSTERED INDEX tmp_exclusions_idx1 ON #tmp_exclusions(elcn_personid);
CREATE NONCLUSTERED INDEX tmp_exclusions_idx2 ON #tmp_exclusions(elcn_organizationid);




--LYBUNT LOGIC
SELECT *
INTO #tmp_lybunt_conributiondonors
FROM 
(
SELECT fecd.elcn_person elcn_personid, NULL elcn_organizationid, 'P' AS person_or_org_ind, elcn_associationtypeidname, elcn_designationname, fecd.elcn_contributiondateutc, fecd.elcn_amount, elcn_recognitioncredit

FROM filteredelcn_contributiondonor fecd
INNER JOIN filteredelcn_contribution fec ON fec.elcn_contributionid = fecd.elcn_contribution
WHERE fecd.statuscode = 1
AND fec.elcn_fiscalyear = @lv_prev1_fy
AND fecd.elcn_person IS NOT NULL
AND fec.elcn_contributiontypename IN (@p_contributiontype)
AND NOT EXISTS (SELECT 'Y'
                FROM filteredelcn_contributiondonor fecd2
				WHERE fecd2.elcn_person = fecd.elcn_person
				AND DATEPART(YEAR,fecd.elcn_contributiondate) = @lv_curr_fy
				)
AND NOT EXISTS (SELECT 'Y'
                FROM #tmp_exclusions tx 
				WHERE tx.elcn_personid = fecd.elcn_person
				)

UNION ALL

SELECT NULL elcn_personid, elcn_organization elcn_organizationid, 'O' AS person_or_org_ind, elcn_associationtypeidname, elcn_designationname, fecd.elcn_contributiondateutc, fecd.elcn_amount, elcn_recognitioncredit

FROM filteredelcn_contributiondonor fecd
INNER JOIN filteredelcn_contribution fec ON fec.elcn_contributionid = fecd.elcn_contribution
WHERE fecd.statuscode = 1
AND fec.elcn_fiscalyear = @lv_prev1_fy
AND fecd.elcn_organization IS NOT NULL
AND NOT EXISTS (SELECT 'Y'
                FROM filteredelcn_contributiondonor fecd2
				WHERE fecd2.elcn_organization = fecd.elcn_organization
				AND DATEPART(YEAR,fecd.elcn_contributiondate) = @lv_curr_fy
				)
AND NOT EXISTS (SELECT 'Y'
                FROM #tmp_exclusions tx 
				WHERE tx.elcn_OrganizationId = fecd.elcn_organization
				)
) fq
;

CREATE NONCLUSTERED INDEX tmp_lybunt_contributiondonors_idx1 ON #tmp_lybunt_conributiondonors(elcn_personid);
CREATE NONCLUSTERED INDEX tmp_lybunt_contributiondonors_idx2 ON #tmp_lybunt_conributiondonors(elcn_organizationid);


SELECT tlc.*
, CRMAF_filteredcontact.fullname
, CRMAF_filteredcontact.elcn_primaryID
, CRMAF_filteredcontact.elcn_primaryconstituentaffiliationidname
, CRMAF_filteredcontact.address1_addresstypecodename
, CRMAF_filteredcontact.address1_line1
, CRMAF_filteredcontact.address1_line2
, CRMAF_filteredcontact.address1_city
, CRMAF_filteredcontact.address1_stateorprovince
FROM  #tmp_lybunt_conributiondonors tlc
INNER JOIN filteredcontact CRMAF_filteredcontact ON tlc.elcn_personid = CRMAF_filteredcontact.contactid

UNION ALL

SELECT tlc.*
, CRMAF_filteredaccount.name
, CRMAF_filteredaccount.elcn_primaryID
, CRMAF_filteredaccount.elcn_primaryconstituentaffiliationidname
, CRMAF_filteredaccount.address1_addresstypecodename
, CRMAF_filteredaccount.address1_line1
, CRMAF_filteredaccount.address1_line2
, CRMAF_filteredaccount.address1_city
, CRMAF_filteredaccount.address1_stateorprovince
FROM  #tmp_lybunt_conributiondonors tlc
INNER JOIN filteredaccount CRMAF_filteredaccount ON tlc.elcn_organizationid = CRMAF_filteredaccount.accountid

</CommandText>
