FROM            FilteredOrganization</CommandText></Query><Fields><Field Name="name"><DataField>name</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="LybuntDataset"><Query><DataSourceName>MSCRM_DataSource</DataSourceName><QueryParameters><QueryParameter Name="@PersonOrganization"><Value>=Parameters!PersonOrganization.Value</Value></QueryParameter></QueryParameters><CommandText>
-- Get boundaries of the current Fiscal Year 
DECLARE @FiscalYear INT
SELECT @FiscalYear = '2019'
DECLARE @fiscalCalendarStart DATETIME
DECLARE @fiscalCalendarStartTemp DATETIME
DECLARE @fiscalMonth INT
DECLARE @fiscalStartDay INT

DECLARE @fiscalYearDisplayCode INT

SELECT @fiscalCalendarStartTemp = fiscalCalendarStart, @fiscalYearDisplayCode = FiscalYearDisplayCode FROM FilteredOrganization

SET @fiscalMonth = MONTH(@fiscalCalendarStartTemp)
SET @fiscalStartDay = DAY(@fiscalCalendarStartTemp)

DECLARE @Today DATE;
SELECT @Today = GETDATE();

-- create a date with the same year as the fiscal year start date for easy comparison
DECLARE @SameYearDate DATE;
IF (MONTH(@Today) = 2 AND DAY(@Today) = 29)
BEGIN
   SELECT @SameYearDate = DATEFROMPARTS(YEAR(@fiscalCalendarStartTemp),  MONTH(@Today), DAY(@Today) -1)
END
ELSE
BEGIN
	SELECT @SameYearDate = DATEFROMPARTS(YEAR(@fiscalCalendarStartTemp),  MONTH(@Today), DAY(@Today))
END

IF (@SameYearDate &lt; @fiscalCalendarStartTemp)
BEGIN
	SELECT @FiscalYear = YEAR(@Today) - 1
END
ELSE
BEGIN
	SELECT @FiscalYear = YEAR(@Today)
END

-- when fiscal year is january 1, it's always treated as BasedOnStartDate not BasedOnEndDate
IF (@fiscalMonth &lt;&gt; 1 OR @fiscalStartDay &lt;&gt; 1) AND @fiscalYearDisplayCode = 2
BEGIN
	SELECT @FiscalYear = @FiscalYear + 1
END 

IF (@fiscalYearDisplayCode = 2) 
	BEGIN
		SELECT @fiscalCalendarStart = DATETIMEFROMPARTS(@FiscalYear - 1, @fiscalMonth, @fiscalStartDay, 00, 00, 00, 01)
	END
ELSE
	BEGIN
		SELECT @fiscalCalendarStart = DATETIMEFROMPARTS(@FiscalYear, @fiscalMonth, @fiscalStartDay, 00, 00, 00, 01)
	END

-- Lybunt/Sybunt
SELECT 	elcn_sortname,
		FullName,
		CAST(elcn_lastcontributiondate AS DATE) AS elcn_lastcontributiondate,
		elcn_lastcontributionamount,
		elcn_lastcontributiontypename,
		elcn_primaryconstituentaffiliationidname,
		contactid as id,
		'contact' as entity
FROM FilteredContact AS CRMAF_Contact
WHERE elcn_lastcontributiondate &lt; @fiscalCalendarStart
AND   elcn_consecutiveyearsofgiving &gt; 0
AND   CHARINDEX('Person', @PersonOrganization) &gt; 0
UNION ALL
SELECT	UPPER(elcn_sortname) AS elcn_sortname,
		Name,
		CAST(elcn_lastcontributiondate AS DATE) AS elcn_lastcontributiondate,
		elcn_lastcontributionamount,
		elcn_lastcontributiontypename,
		elcn_primaryconstituentaffiliationidname,
		accountid as id,
		'account' as entity
FROM FilteredAccount CRMAF_Account
WHERE elcn_lastcontributiondate &lt; @fiscalCalendarStart
AND   elcn_consecutiveyearsofgiving &gt; 0
AND   CHARINDEX('Organization', @PersonOrganization) &gt; 0
ORDER BY elcn_sortname;
</CommandText></Query><Fields><Field Name="elcn_sortname"><DataField>elcn_sortname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_lastcontributiondate"><DataField>elcn_lastcontributiondate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="elcn_lastcontributionamount"><DataField>elcn_lastcontributionamount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="elcn_lastcontributiontypename"><DataField>elcn_lastcontributiontypename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="elcn_primaryconstituentaffiliationidname"><DataField>elcn_primaryconstituentaffiliationidname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="id"><DataField>id</DataField><rd:TypeName>System.Guid</rd:TypeName></Field><Field Name="entity"><DataField>entity</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="PersonOrganization"><DataType>String</DataType><DefaultValue><Values><Value>PersonOrganization</Value></Values></DefaultValue><Prompt>Include</Prompt><ValidValues><ParameterValues><ParameterValue><Value>PersonOrganization</Value><Label>People and Organizations</Label></ParameterValue><ParameterValue><Value>Person</Value><Label>People only</Label></ParameterValue><ParameterValue><Value>Organization</Value><Label>Organizations only</Label></ParameterValue></ParameterValues></ValidValues></ReportParameter><ReportParameter Name="CRM_FilterText"><DataType>String</DataType><Nullable>true</Nullable><AllowBlank>true</AllowBlank><Prompt>CRM_FilterText</Prompt><Hidden>true</Hidden></ReportParameter><ReportParameter Name="CRM_URL"><DataType>String</DataType><AllowBlank>true</AllowBlank><Prompt>CRM_URL</Prompt><Hidden>true</Hidden></ReportParameter><ReportParameter Name="CRM_ShortDatePattern"><DataType>String</DataType><DefaultValue><Values><Value>M/d/yyyy</Value></Values></DefaultValue><Prompt>CRM_ShortDatePattern</Prompt><Hidden>true</Hidden></ReportParameter></ReportParameters><CustomProperties><CustomProperty><Name>Custom</Name><Value>&lt;MSCRM xmlns="mscrm"&gt;&amp;lt;ReportFilter&amp;gt;&amp;lt;ReportEntity paramname="P1"&amp;gt;&amp;lt;fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="false"&amp;gt;&amp;lt;entity name="contact"&amp;gt;&amp;lt;all-attributes /&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&amp;lt;/ReportEntity&amp;gt;&amp;lt;ReportEntity paramname="P2"&amp;gt;&amp;lt;fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="false"&amp;gt;&amp;lt;entity name="account"&amp;gt;&amp;lt;all-attributes /&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&amp;lt;/ReportEntity&amp;gt;&amp;lt;/ReportFilter&amp;gt;&lt;/MSCRM&gt;</Value></CustomProperty></CustomProperties><Code>Function HTMLDecode(byVal EncodedString) As String
  Dim DecodedString, i
  DecodedString = EncodedString
  DecodedString = Replace(DecodedString, "&amp;quot;", chr(34) )
  DecodedString = Replace(DecodedString, "&amp;lt;"  , chr(60) )
  DecodedString = Replace(DecodedString, "&amp;gt;"  , chr(62) )
  DecodedString = Replace(DecodedString, "&amp;amp;" , chr(38) )
  DecodedString = Replace(DecodedString, "&amp;nbsp;", chr(32) )
  For i = 1 to 255
    DecodedString = Replace(DecodedString, "&amp;#" &amp; i &amp; ";", chr( i ) )
  Next
  Return DecodedString
End Function</Code><EmbeddedImages><EmbeddedImage Name="EllucianCrmAdvanceLogoReports"><MIMEType>image/png</MIMEType><ImageData>




